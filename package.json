{
  "name": "yaseer",
  "version": "0.0.4",
  "description": "a simple JavaScript library",
  "type": "module",
  "types": "./y/yaseer.utils.d.ts",
  "files": [
    "y"
  ],
  "main": "./y/yaseer.util.umd.cjs",
  "module": "./y/yaseer.utils.js",
  "exports": {
    ".": {
      "import": "./y/yaseer.utils.js",
      "require": "./y/yaseer.util.umd.cjs"
    }
  },
  "scripts": {
    "test": "node index.spec.mjs",
    "build": "tsc --noEmit && vite build",
    "watch": "vite build --watch",
    "build:types": "tsc -m amd -t ESNext -d --isolatedModules false  --moduleResolution node --esModuleInterop --outFile y/yaseer.utils.js src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fahdfady/yaseer.git"
  },
  "author": "fahddev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fahdfady/yaseer/issues"
  },
  "homepage": "https://github.com/fahdfady/yaseer#readme",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "path": "^0.12.7",
    "typescript": "^5.4.2",
    "vite": "^5.1.5"
  },
  "dependencies": {
    "vite-plugin-dts": "^3.7.3"
  }
}
